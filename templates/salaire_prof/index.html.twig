{% extends 'layout.html.twig' %}


{% block body %}

    <h1>SalaireProf index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>TauxHoraires</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for salaire_prof in salaire_profs %}
            <tr>
                <td>{{ salaire_prof.id }}</td>
                <td>{{ salaire_prof.tauxHoraires }}</td>
                <td>
                    <a href="{{ path('salaire_prof_show', {'id': salaire_prof.id}) }}">show</a>
                    <a href="{{ path('salaire_prof_edit', {'id': salaire_prof.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('salaire_prof_new') }}">Create new</a>
{% endblock %}
